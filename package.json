{
  "name": "fuse-bridge-ui",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npx tailwindcss -i src/styles/index.css -o dist/output.css --watch\" \"vite\"",
    "build": "tsc && npx tailwindcss -i src/styles/index.css -o dist/output.css && vite build",
    "preview": "vite preview",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx src",
    "lint:fix": "eslint --ext .js,.jsx,.ts,.tsx src --fix",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@layerzerolabs/ui-app-config": "^0.1.6",
    "@reduxjs/toolkit": "^1.9.5",
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "@web3-onboard/coinbase": "^2.2.5",
    "@web3-onboard/core": "^2.20.4",
    "@web3-onboard/injected-wallets": "^2.10.2",
    "@web3-onboard/ledger": "^2.5.1",
    "@web3-onboard/react": "^2.8.9",
    "@web3-onboard/torus": "^2.2.5",
    "@web3-onboard/trezor": "^2.4.2",
    "@web3-onboard/walletconnect": "^2.4.2",
    "concurrently": "^8.2.0",
    "ethers": "^6.6.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.1.1",
    "react-router-dom": "^6.14.1",
